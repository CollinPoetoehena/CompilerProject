void foo() {

    // Success
    for (int i = 0, 10) {
        i = 10;
    }
    for (int i = 0, 10)
        i = 10;
    for (int i = 10, 0, -1)
        i = 10;
    for (int i = 0, 10, 1)
        i = 10;

    // Failure 
    for (int i = 0, true) {
        i = 10;
    }
    for (int i = 0, 5.3463)
        i = 10;
    for (int i = 10, 0, -1.54434)
        i = 10;
    for (int i = 0, 10, 1.6457456)
        i = 10;
    for (int i = 0, 10, true)
        i = 10;
}