extern void printInt( int val);
extern void printSpaces( int num);

// TODO: remove when milestone 14 is supported, multiple modules
export int fib(int n) {
    if (n <= 1) return n;
    return fib(n - 1) + fib(n - 2);
}

export void fibs(int n) {
    int count = 0;

    while (n != count) {
        printInt(fib(count));
        printSpaces(1);
        count = count + 1;
    }
}

