extern void printInt(int val);
extern void printSpaces(int num);
extern void printNewlines(int num);

// Milestone 14 is not supported (multiple modules), so use the function here to test this file!
export int fib(int n) {
    if (n <= 1) return n;
    return fib(n - 1) + fib(n - 2);
}

export void fibs(int n) {
    int count = 0;

    while (n != count) {
        printInt(fib(count));
        printSpaces(1);
        count = count + 1;
    }
}

// No nested functions in basic, so no need to test that, the below is the content without that, so still test it,
// can be helpful for testing the compiler!
bool isprime(int n) {
    if (n <= 1) return false;

    for (int i = 2, n) {
        if (n % i == 0) return false;
    }
    
    return true;
}

export void primes(int n) {
    int count = 0;
    int i = 0;

    while (n != count) {
        if (isprime(i)) {
            count = count + 1;
            printInt(i);
            printSpaces(1);
        } 
        i = i + 1;
    }
}

export int main() {
    // coreio.cvc tests
    fibs(5);
    printNewlines(1);

    primes(6);
    printNewlines(3);

    return 0;
}