extern int gcd(int a, int b) {
    for(int i = a, 0, -1) {
        if(a % i == 0 && b % i == 0) return i;
    }

    return 1;
}

int fac(int n) {
    if (n == 0) return 1;
    return n * fac(n - 1);
}

int fib(int n) {
    if (n <= 1) return n;
    return fib(n - 1) + fib(n - 2);
}

bool isprime(int n) {
    if (n <= 1) return false;

    for (int i = 2, n) {
        if (n % i == 0) return false;
    }
    
    return true;
}